{
  "name": "blacklisted-usdt-address-agent",
  "version": "0.0.1",
  "description": "Forta agent that checks if a blacklisted address is involved in *any* transaction",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec 'forta-agent run'",
    "start:prod": "forta-agent run --prod",
    "test": "jest"
  },
  "dependencies": {
    "forta-agent": "^0.0.8",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12"
  }
}
